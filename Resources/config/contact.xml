<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="black_person.contact.form.type">Black\Bundle\PersonBundle\Form\Type\ContactType</parameter>
    </parameters>

    <services>
        <service id="black_person.contact.form.type" class="%black_person.contact.form.type%">
            <argument>%black_person.person.model.class%</argument>
            <tag name="form.type" alias="black_person_contact" />
        </service>
    </services>
</container>
